{
"version": 2,
"builds": [{ "src": "server.js", "use": "@now/node-server" }],
"routes": [
    {
      "src": "/",
      "dest": "server.js",
      "methods": ["GET"]
    },
    {
        "src": "/updatedb/(.*)*",
        "dest": "server.js",
        "methods": ["GET"]
    },
    {
        "src": "/search/(.*)*",
        "dest": "server.js",
        "methods": ["GET"]
    }
  ]
}